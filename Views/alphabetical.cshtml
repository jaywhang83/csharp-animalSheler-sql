<h1>Here are animals by alphabetical order</h1>
@if(@Model.Count ==0)
{
  <p>There are no animals.</p>
}
else
{
  @foreach(var animal in Model)
  {
    <p>@animal.GetName()</p>
      <ul>
        <li>Gender: @animal.GetGender()</li>
        <li>Date of admittance: @animal.GetDate()</li>
        <li>Breed: @animal.GetBreed()</li>
      </ul>
  }
}
<p><a href="/animals">View all animals</a></p>
<p><a href="/animals/breed">View animals by breed</a></p>
</p><a href="/">Return home</a></p>
